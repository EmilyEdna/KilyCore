    <script>
        //配置
        var options = {
            Init: function () {
                this.InitDom();
                this.InitEvents();
            },
            //元素
            InitDom: function () {
                //获取树
                options.GetMenu();
                //安全退出
                options.SignOut();
                //用户信息
                $("body").GetCompanyInfo();
                //显示登录
                $(".text-muted").prepend(CompanyUser.CompanyName);
                $(".font-bold").prepend(CompanyUser.CompanyAccount);
            },
            //获取树
            GetMenu: function () {
                controller.ajax({
                    url: "Enterprise/GetEnterpriseMenu",
                    data: {},
                    type: "post",
                    async: false,
                    xhrFields: {
                        withCredentials: true
                    },
                    crossDomain: true,
                    success: function (result) {
                        $('#side-menu').SetTree(result.data);
                    }
                });
            },
            //安全退出
            SignOut: function () {
                $(".LoginOut").click(function () {
                    controller.ClearLocal();
                    controller.ajax({
                        url: "Enterprise/LoginOut",
                        data: {},
                        type: "post",
                        success: function (result) {
                            controller.Confirm("您已经成功安全退出系统！", function (index) {
                                controller.SetHref("Login");
                            })
                        }
                    });
                    controller.DeleteCookie();
                });
            },
            //事件
            InitEvents: function () { },
            //行为
            action: {}
        };
        //初始化
        $(document).ready(function () {
            options.Init();
        });
    </script>
