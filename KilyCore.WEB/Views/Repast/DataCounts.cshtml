        .select:hover {
            box-shadow: 0 1px 4px rgba(0,0,0,.15)
        }

        .select:before {
            content: "";
            position: absolute;
            width: 0;
            height: 0;
            border: 10px solid transparent;
            border-top-color: #ccc;
            top: 14px;
            right: 10px;
            cursor: pointer;
            z-index: -2
        }

        .select select {
            cursor: pointer;
            padding: 10px;
            width: 100%;
            border: none;
            background: 0 0;
            background-image: none;
            -webkit-appearance: none;
            -moz-appearance: none
        }

            .select select:focus {
                outline: 0
            }

    .fixed {
        position: fixed;
        right: 5px;
        top: 300px;
        padding: 0 12px;
        z-index: 201;
        border-radius: 6px;
        background-color: #fff;
        box-shadow: 0 2px 10px rgba(0,0,0,.5)
    }

        .fixed a {
            font-size: 14px;
            display: block;
            width: 100%;
            padding: 12px 0;
            text-align: center;
            color: #2589ff
        }

            .fixed a:nth-child(1), .fixed a:nth-child(2), .fixed a:nth-child(3) {
                border-bottom: 1px solid #91b4ff
            }

            .fixed a.hover-action:hover ~ .hidden {
                display: block
            }

        .fixed .hidden {
            z-index: 201;
            display: none;
            position: absolute;
            right: 108px;
            top: 73px;
            width: 100px;
            height: 100px;
            box-shadow: 0 2px 10px rgba(0,0,0,.5)
        }

    .hidden img {
        width: 100px;
        height: 100px
    }

    .hidden::after {
        position: absolute;
        right: -12px;
        top: 46px;
        content: "";
        display: block;
        width: 0;
        height: 0;
        border: 6px solid #fff;
        border-color: transparent transparent transparent #fff
