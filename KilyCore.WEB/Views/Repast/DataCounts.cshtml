    .select {
        display: inline-block;
        position: relative;
        vertical-align: middle;
        height: 36px;
        line-height: 36px;
        padding: 0;
        overflow: hidden;
        background-color: #fff;
        color: #555;
        border: 1px solid #aaa;
        text-shadow: none;
        border-radius: 4px;
        transition: box-shadow .25s ease;
        text-align: center;
        z-index: 2
