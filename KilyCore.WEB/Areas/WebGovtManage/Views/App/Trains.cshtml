@{
    ViewData["Title"] = "培训通知";
    Layout = "~/Views/Shared/BaseView.cshtml";
}
<link href="http://old.cfda.vip/Cashier/MUI/mui.min.css" rel="stylesheet" />
<script type="text/javascript">
    var options = {
        Init: function () {
             this.InitDom();
        },
        InitDom: function () {
            var Id = controller.GetParam("Id");
             var ApiKey = controller.GetParam("ApiKey");
            var Syskey = controller.GetParam("SysKey");
            var Token = controller.GetParam("Token");
            var obj = { "RSAToKen": Token, "RSAApiKey": ApiKey, "RSASysKey": Syskey };
            controller.SetCookie(obj);
            controller.ajax({
                url: "GovtWeb/GetTrainNoticeDetail",
                data: {Id:Id},
                type: "post",
                async: false,
                success: function (result) {
                    controller.SetLableValue("#Detail", result.data);
                },
            });
        }
    };
     //页面初始化
    $(document).ready(function () {
        options.Init();
    });
</script>
<div class="mui-collapse-content" id="Detail">
    <ul class="mui-table-view">
        <li class="mui-table-view-cell">
            <h5 class="mui-ellipsis mui-pull-left" style="color: #333;">培训科目：</h5>
            <h5 class="mui-ellipsis mui-pull-right" style="color: #999;" name="TrainTitle" type="txt"></h5>
        </li>
        <li class="mui-table-view-cell">
            <h5 class="mui-ellipsis mui-pull-left" style="color: #333;">培训地点：</h5>
            <h5 class="mui-ellipsis mui-pull-right" style="color: #999;"  name="TrainPlace" type="txt"></h5>
        </li>
        <li class="mui-table-view-cell">
            <h5 class="mui-ellipsis mui-pull-left" style="color: #333;">培训时间：</h5>
            <h5 class="mui-ellipsis mui-pull-right" style="color: #999;" name="TrainTime" type="txt"></h5>
        </li>
        <li class="mui-table-view-cell">
            <h5 class="mui-ellipsis mui-pull-left" style="color: #333;">培训概要：</h5>
            <h5 class="mui-ellipsis mui-pull-right" style="color: #999;" name="Desc" type="txt"></h5>
        </li>
        <li class="mui-table-view-cell">
            <h5 class="mui-ellipsis mui-pull-left" style="color: #333;">培训内容：</h5>
            <h5 class="mui-ellipsis mui-pull-right" style="color: #999;"></h5>
        </li>
        <li class="mui-table-view-cell mui-media">
            <span href="javascript:;">
                <div class="mui-media-body">
                    <p name="Remark" type="txt"></p>
                </div>
            </span>
        </li>
    </ul>
</div>

