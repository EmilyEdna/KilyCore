
@{
    ViewData["Title"] = "产品详情";
    Layout = "~/Views/Shared/BaseView.cshtml";
}
<link href="http://old.cfda.vip/Cashier/MUI/mui.min.css" rel="stylesheet" />
<script type="text/javascript">
    var options = {
        Init: function () {
             this.InitDom();
        },
        InitDom: function () {

        }
    };
     //页面初始化
    $(document).ready(function () {
        options.Init();
    });
</script>
<div class="mui-collapse-content">
    <ul class="mui-table-view">
        <li class="mui-table-view-cell">
            <h5 class="mui-ellipsis mui-pull-left" style="color: #333;">商品编号：</h5>
            <h5 class="mui-ellipsis mui-pull-right" style="color: #999;"></h5>
        </li>
        <li class="mui-table-view-cell">
            <h5 class="mui-ellipsis mui-pull-left" style="color: #333;">商品名称：</h5>
            <h5 class="mui-ellipsis mui-pull-right" style="color: #999;"></h5>
        </li>
        <li class="mui-table-view-cell">
            <h5 class="mui-ellipsis mui-pull-left" style="color: #333;">商品规格：</h5>
            <h5 class="mui-ellipsis mui-pull-right" style="color: #999;"></h5>
        </li>
        <li class="mui-table-view-cell">
            <h5 class="mui-ellipsis mui-pull-left" style="color: #333;">保质期：</h5>
            <h5 class="mui-ellipsis mui-pull-right" style="color: #999;"></h5>
        </li>
        <li class="mui-table-view-cell">
            <h5 class="mui-ellipsis mui-pull-left" style="color: #333;">生产厂商：</h5>
            <h5 class="mui-ellipsis mui-pull-right" style="color: #999;"></h5>
        </li>
        <li class="mui-table-view-cell">
            <h5 class="mui-ellipsis mui-pull-left" style="color: #333;">厂家热线：</h5>
            <h5 class="mui-ellipsis mui-pull-right" style="color: #999;"></h5>
        </li>
        <li class="mui-table-view-cell">
            <h5 class="mui-ellipsis mui-pull-left" style="color: #333;">厂家地址：</h5>
            <h5 class="mui-ellipsis mui-pull-right" style="color: #999;"></h5>
        </li>
        <li class="mui-table-view-cell" style="background-color:#999;text-align:center;color:#fff;margin:6px 0px;">产品介绍</li>
        <li class="mui-table-view-cell mui-media">
            <span href="javascript:;">
                <div class="mui-media-body">
                    <p class='mui-ellipsis'><%=entity.content %></p>
                </div>
            </span>
        </li>
    </ul>
</div>

