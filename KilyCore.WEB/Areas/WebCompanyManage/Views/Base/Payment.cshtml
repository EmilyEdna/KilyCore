
@{
    ViewData["Title"] = "合同缴费";
    Layout = "~/Views/Shared/BaseView.cshtml";
}
@section css{
}
@section script{
    <script>
        //配置
        var options = {
            Init: function () {
                this.InitDom();
                this.InitEvents();
            },
            //元素
            InitDom: function () {
                var Guid = controller.GetParam("Id");
                options.GetField(Guid);
            },
            //取详情
            GetField: function (data) {

            },
            //事件
            InitEvents: function () {
                //验证
                controller.ValidateConfirm("#Edit");
            },
            //行为
            action: {}
        };
        //初始化
        $(document).ready(function () {
            options.Init();
        });
    </script>
}
<div class="ibox float-e-margins">
    <div class="ibox-content">
        <form class="orm-horizontal" method="post" action="EnterpriseWeb/EditDic" id="Edit">
            <input type="hidden" name="Id" />
            <input type="hidden" name="CompanyId" id="CompanyId" />
            <table class="table table-hover table-bordered">
                <tr>
                    <th><label class="control-label">合同模式</label></th>
                    <td class="has-warning" colspan="3">
                        <label class="radio-inline">
                            <input type="radio" name="SupplierType" value="线上合同" style="margin-top:3px" checked />
                            <text title="线上合同">线上合同</text>
                        </label>
                        <label class="radio-inline">
                            <input type="radio" name="SupplierType" value="线下合同" style="margin-top:3px" />
                            <text title="线下合同">线下合同</text>
                        </label>
                    </td>
                </tr>
            </table>
        </form>
    </div>
</div>